<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cs442.team_2.sudoku"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="23" />

    <!-- <uses-sdk android:minSdkVersion="3" /> -->
    <application
        android:icon="@drawable/sudoku"
        android:label="@string/app_name" >
        <activity
            android:name="com.cs442.team_2.sudoku.SplashScreen"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Black.NoTitleBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.cs442.team_2.sudoku.Sudoku"
            android:label="@string/app_name" />
        <activity
            android:name="com.cs442.team_2.sudoku.About"
            android:label="@string/about_title"
            android:theme="@android:style/Theme.Dialog" />
        <activity
            android:name="com.cs442.team_2.sudoku.Prefs"
            android:label="@string/settings_title" />
        <activity
            android:name="com.cs442.team_2.sudoku.SudokuListActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.cs442.team_2.sudoku.SudokuRecords"
            android:label="@string/app_name"
            android:theme="@style/Theme.AppCompat" >
        </activity>
        <activity
            android:name="com.cs442.team_2.sudoku.SudokuPlayActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" >
        </activity>
        <activity android:name="com.cs442.team_2.sudoku.TutorialActivity" />
    </application>

</manifest>